<!-- QQ邮箱 -->
  <prescription type="activity" package="com.tencent.androidqqmail" class="com.tencent.qqmail.launcher.third.LaunchComposeNote" sender="other">
    <intent-filter>
      <action name="android.intent.action.VIEW"/>
      <cat name="android.intent.category.DEFAULT"/>
      <cation name="android.intent.action.SENDTO"/>
      <cat name="android.intent.category.BROWSABLE"/>
      <scheme name="mailto"/>
